<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
    <title>Add New Blog Page</title>

</head>
<body>
    <%- include('./partials/nav') %>
    <form action="/note" method="post">
        <div class="container ">
            <div class="mt-3">
                <label for="title" class="form-label">Title</label>
                <input type="text" class="form-control" id="title" name="title" required>
              </div>
              <div class="form-floating mt-4">
                <textarea class="form-control"  id="floatingTextarea2" name="body" style="height: 100px" required></textarea>
                <label for="floatingTextarea2">Write Your Note</label>
              </div>
              <div class="mt-3">
                <div class="form-floating mt-4">
                  <textarea class="form-control"  id="floatingTextarea2" name="label" style="height: 30px" value="default" required>default</textarea>
                  <label for="floatingTextarea2">Enter label</label>
                  <button class="btn btn-primary" class="mt-3" type="submit">Make Note</button>
                </div>
              </div>
        </div>
    </form>
    <%- include('./partials/scripts') %>
</body>
</html>