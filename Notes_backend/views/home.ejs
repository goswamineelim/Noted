<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
    <title>Homepage</title>

</head>
<body>
    <%- include('./partials/nav') %>
    <div class="container mt-3 mx-auto row row-cols-4 ">
        <% notes.forEach(note => { %>
            <div class="col">
                <div class="card" style="width: auto;">
                    <div class="card-body">
                      <h5 class="card-title"><%= note.title %></h5>
                      <a href="/note/<%= note._id %>" class="btn btn-primary">View</a>
                    </div>
                  </div>
            </div>
        <% }) %>
    </div>
    
    <%- include('./partials/scripts') %>
</body>
</html>